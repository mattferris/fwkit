#!/usr/sbin/fwksrv
# fwkit - tftp.service - Trivial File Transfer Protocol; 69/udp

services:
  tftp:
    description: TFTP (Trivial File Transfer Protocol)
    modprobe: nf_conntrack_tftp
    transports:
      -
        protocol: udp
        listen_port: 69
      -
        protocol: udp
        listen_port: 1024:65525
